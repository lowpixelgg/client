{
  "name": "low-launcher",
  "productName": "LowPixel Client",
  "private": true,
  "version": "2.5.4",
  "description": "A client for LowPixel",
  "author": "lowpixel.g(flashii, evol, z4, viniciuss) <buss.vitorsheik@gmail.com>",
  "license": "MIT",
  "main": "dist/electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.6",
    "@types/adm-zip": "^0.5.3",
    "@types/unzipper": "^0.10.8",
    "adm-zip": "^0.5.10",
    "async": "^3.2.4",
    "axios": "^1.4.0",
    "better-queue": "^3.8.12",
    "chalk": "^5.3.0",
    "child_process": "^1.0.2",
    "cross-spawn": "^7.0.3",
    "edit-json-file": "^1.7.0",
    "fetch": "^1.1.0",
    "framer-motion": "^7.4.0",
    "from": "^0.1.7",
    "http": "^0.0.1-security",
    "import": "^0.0.6",
    "is-admin": "^4.0.0",
    "leaflet": "^1.9.4",
    "localstorage-slim": "^2.4.0",
    "mtasa-informations": "^1.1.3",
    "progress-stream": "^2.0.0",
    "react-icons": "^4.4.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.4.0",
    "react-three-fiber": "^6.0.13",
    "react-toastify": "^9.1.3",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.7.1",
    "styled-components": "^5.3.5",
    "swr": "^2.1.3",
    "unzipper": "^0.10.14",
    "utils": "^0.3.1",
    "yauzl": "^2.10.0",
    "zip-lib": "^0.7.3"
  },
  "devDependencies": {
    "@types/better-queue": "^3.8.3",
    "@types/discord-rpc": "^4.0.4",
    "@types/edit-json-file": "^1.7.0",
    "@types/leaflet": "^1.9.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.0.1",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.4",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "vite-plugin-electron": "^0.9.2",
    "vite-plugin-mkcert": "^1.16.0"
  },
  "build": {
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extends": null,
    "appId": "com.lowpixel.app",
    "productName": "LowPixel Client",
    "copyright": "Copyright 2024 LowPixel",
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "rpm"
      ]
    },
    "win": {
      "icon": "resources/icone.png",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "license": "resources/LICENSE.txt",
      "include": "resources/installer.nsh",
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "displayLanguageSelector": true,
      "installerLanguages": [
        "en_US",
        "pt_BR",
        "ru_RU",
        "es_ES"
      ],
      "deleteAppDataOnUninstall": false
    }
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_HOSTNAME": "127.0.0.1",
      "VITE_DEV_SERVER_PORT": 7777,
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777"
    }
  }
}
